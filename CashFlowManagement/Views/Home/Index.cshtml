<div id="create-new-income-modal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Create new income</h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal">
                    <div class="form-group">
                        <label class="control-label col-sm-3" for="email">Income:</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control" id="income" placeholder="">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3" for="pwd">Monthly income:</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control" id="monthly-income" placeholder="">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3" for="pwd">Original value:</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control" id="monthly-income" placeholder="">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3" for="pwd">Mortgage value:</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control" id="monthly-income" placeholder="">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3" for="pwd">Expense Interest:</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control" id="monthly-income" placeholder="">
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer text-center">
                <button type="button" class="btn btn-success" data-dismiss="modal">Create</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>